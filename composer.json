{
  "name": "tlapnet/notifications",
  "description": "Sending notifications",
  "keywords": [
    "nette",
    "notifications"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/tlapnet/notifications",
  "authors": [
    {
      "name": "Filip Šuška",
      "homepage": "https://github.com/vody105"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "contributte/di": "^0.4.0",
    "mockery/mockery": "^1.1.0",
    "ninjify/qa": "^0.9.0",
    "phpstan/extension-installer": "^1.0.0",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-nette": "^0.11.0",
    "phpstan/phpstan-shim": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "phpunit/phpunit": "^8.0.0",
    "tracy/tracy": "~2.5.0"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Notifications\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Tlapnet\\Notifications\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
